#!/usr/bin/python3

def square_matrix_simple(matrix=[]):
    nCount = 0
    nRes = 0
    for insideList in matrix:
        nCount = len(insideList)
        for val in insideList:
            nRes += val**2

    return nRes


matrix = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]
print(square_matrix_simple(matrix))
